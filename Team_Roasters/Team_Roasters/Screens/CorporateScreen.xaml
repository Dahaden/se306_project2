            	<s:SurfaceButton 
            		x:Name="what_we_do" 
            		Content="What We Do" 
                    Click="WhatWeDo_Click"
            		Background="#FF8ECADC" FontSize="40"  Style="{DynamicResource HomeContentButton}" Margin="0" Height="171.984" VerticalAlignment="Top" FontFamily="Gill Sans MT" Foreground="Black" FontWeight="Bold" FontStyle="Italic" >
            		<s:SurfaceButton.Effect>
            			<DropShadowEffect BlurRadius="10" Opacity="0.6"/>
            		</s:SurfaceButton.Effect>
            	</s:SurfaceButton>
            	<s:SurfaceButton 

            		x:Name="family_and_support" 
            		Click="Family_Click"
            		Content="Family and Support"
            		Background="#FFFDC37D" FontSize="40" Style="{DynamicResource HomeContentButton}" Margin="0" Foreground="Black" FontFamily="Gill Sans MT" FontWeight="Bold" FontStyle="Italic" Height="171.984">
            		<s:SurfaceButton.Effect>
            			<DropShadowEffect BlurRadius="10" Opacity="0.6"/>
