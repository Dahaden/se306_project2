            	<Button Content="" Margin="155.523,82.562,190.977,0" Height="152.499" VerticalAlignment="Top" Grid.Column="1" BorderThickness="0" BorderBrush="{x:Null}">
            		<Button.Background>
            			<ImageBrush Stretch="Uniform" ImageSource="Gold-Buyers.jpg"/>
            		</Button.Background>
            	</Button>
            	<Button Content="" Margin="232,0,228.392,136.048" Grid.Column="1" Height="143.201" VerticalAlignment="Bottom" BorderThickness="0" BorderBrush="{x:Null}">
            		<Button.Background>
            			<ImageBrush Stretch="Uniform" ImageSource="Farmers-Logo-Hm-Page.jpg"/>
