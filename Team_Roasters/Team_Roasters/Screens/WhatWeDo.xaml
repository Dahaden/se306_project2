    <view:Screen.Resources>
        <Storyboard x:Key="Shrink">
            <DoubleAnimation BeginTime="0:0:0" Duration="0:0:1" Storyboard.TargetName="MainPage" Storyboard.TargetProperty="RenderTransform.ScaleX" From="1" To="0"/>
            <DoubleAnimation BeginTime="0:0:0" Duration="0:0:1" Storyboard.TargetName="MainPage" Storyboard.TargetProperty="RenderTransform.ScaleY" From="1" To="0"/>
        </Storyboard>

        <Storyboard x:Key="Grow">
            <DoubleAnimation BeginTime="0:0:0" Duration="0:0:1" Storyboard.TargetName="MainPage" Storyboard.TargetProperty="RenderTransform.ScaleX" From="0" To="1"/>
            <DoubleAnimation BeginTime="0:0:0" Duration="0:0:1" Storyboard.TargetName="MainPage" Storyboard.TargetProperty="RenderTransform.ScaleY" From="0" To="1"/>
